<h1> Sign Up! </h1>

<form action= '<%= users_url %>' method="post">

  <input type = "hidden"
  name = "authenticity_token"
  value '<%= form_authenticity_token %>' >

  <label>Email:
    <input type="text" name="user[email]">
  </label>
  <br><br>
  <label>Password:
    <input type="password" name="user[password]">
  </label>
  <br><br>
  <input type="submit" value="Sign Up">
</form>
